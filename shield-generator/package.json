{
  "name": "shield-generator",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/tejashah88/devpost-shields",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --cname devpost-shields.com"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "buefy": "npm:@ntohq/buefy-next@^0.1.4",
    "is-url": "^1.2.4",
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/is-url": "^1.2.32",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.28.0",
    "gh-pages": "^6.2.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.79.5",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vue-tsc": "^2.1.6"
  }
}
